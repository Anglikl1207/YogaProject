{
  "name": "eleventy-basic",
  "version": "1.0.0",
  "description": "Your Site Description Goes Here",
  "main": "index.html",
  "scripts": {
    "watch:sass": "sass --watch src/sass:public/css",
    "watch:eleventy": "npx @11ty/eleventy --serve --quiet",
    "build:sass": "sass src/sass:public/css",
    "build:eleventy": "npx @11ty/eleventy",
    "postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass build:eleventy"
  },
  "keywords": [],
  "author": "Chris and Angelica",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "sass": "^1.43.4"
  }
}
